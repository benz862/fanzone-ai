{
    "buildCommand": "vite build",
    "outputDirectory": "dist",
  
    "functions": {
      "api/*.js": {
        "runtime": "nodejs20.x"
      }
    },
  
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }